{
  "name": "simple-react-pagination-js",
  "version": "0.0.0-semantically-release",
  "description": "simple-pagination",
  "main": "build/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/aniltako/simple-react-pagination-js.git"
  },
  "homepage": "https://aniltako.github.io/simple-react-pagination-js/",
  "scripts": {
    "build-custom": "node-sass-chokidar --include-path ./node_modules src/styles/sass/custom -o src/styles/css/custom --output-style compressed",
    "build-bootstrap": "node-sass-chokidar --include-path ./node_modules src/styles/sass/bootstrap -o src/styles/css/bootstrap/ --output-style compressed",
    "watch-custom": "node-sass-chokidar --include-path ./node_modules src/styles/sass/custom/style.min.scss -o src/styles/css/custom/ --watch --recursive --output-style compressed",
    "watch-bootstrap": "node-sass-chokidar --include-path ./node_modules src/styles/sass/bootstrap/bootstrap.min.scss -o src/styles/css/bootstrap/ --watch --recursive --output-style compressed",
    "start-dev": "webpack-dev-server --config webpack.dev.config.js",
    "start-demo": "webpack-dev-server --config webpack.demo.config.js",
    "start": "npm-run-all -p watch-bootstrap watch-custom start-dev",
    "github": "webpack --config webpack.github.config.js",
    "build-github": "gh-pages -d github",
    "build-prod": "webpack --config webpack.prod.config.js",
    "clear-build": "rimraf build",
    "build": "npm-run-all clear-build build-custom build-prod",
    "deploy": "npm-run-all clear-build build-custom build-github",
    "demo": "npm-run-all -p watch-bootstrap watch-custom start-demo",
    "test": "jest --verbose",
    "commit": "git-cz",
    "semantic-release": "semantic-release"
  },
  "files": [
    "build"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.0.6",
    "chalk": "^2.4.2",
    "css-loader": "^3.1.0",
    "cz-conventional-changelog": "^3.0.2",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "enzyme-to-json": "^3.4.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^4.2.0",
    "gh-pages": "^2.1.1",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^24.8.0",
    "mini-css-extract-plugin": "^0.8.0",
    "node-sass": "^4.12.0",
    "node-sass-chokidar": "^1.3.5",
    "npm-run-all": "^4.1.5",
    "prop-types": "^15.7.2",
    "react-hot-loader": "^4.12.9",
    "react-test-renderer": "^16.8.6",
    "rimraf": "^3.0.0",
    "sass-loader": "^8.0.0",
    "semantic-release": "^15.13.24",
    "style-loader": "^1.0.0",
    "webpack": "^4.36.1",
    "webpack-cli": "^3.3.6",
    "webpack-dev-server": "^3.7.2",
    "webpack-merge": "^4.2.1"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "release": {
    "branches": [
      "master",
      "next"
    ]
  },
  "keywords": [
    "js",
    "simple",
    "react",
    "reactjs",
    "pagination",
    "paginator",
    "bootstrap",
    "webpack",
    "babel",
    "dark",
    "theme",
    "react-transform",
    "react-component"
  ]
}
